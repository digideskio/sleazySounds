<html lang-"en" ng-app="app">
<head>

</head>
<body>
<% if @tracks %>
  <% @tracks.each do |track| %>
    <li> 
    <a href="<%= track.object %>"> <%=track.title %> </a>
    <%= link_to "X", track_path(track), method: :delete %>
    </li>
  <% end %>
<% end %>


<footer>
<center>
      <div class="options">
        <a ng-click="stopSong()" id="pause"><strong>&#9616;&#9616;</strong></a>
        <div id="back">
        <% if user_signed_in? %>
    
          <%= link_to "ï¿©", tracks_path %>
        <% end %>
        </div>
      </div>
      </center>
    </footer>

</body>
</html>
